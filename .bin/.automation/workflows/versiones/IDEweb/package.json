{
  "name": "ideweb-metaverso-workflows",
  "version": "0.1.0",
  "private": true,
  "description": "IDE web React para edición y gestión de workflows versionados del metaverso.",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently \"npm run start\" \"npm run server\"",
    "server": "nodemon server/index.js",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css,md}",
    "type-check": "tsc --noEmit",
    "analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "test:coverage": "npm test -- --coverage --watchAll=false",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "deploy": "npm run build && npm run deploy:upload",
    "deploy:upload": "aws s3 sync build/ s3://metaverso-ideweb --delete",
    "docker:build": "docker build -t ideweb-metaverso .",
    "docker:run": "docker run -p 3000:3000 ideweb-metaverso",
    "workflow:analyze": "node ../version-manager.js analyze-all",
    "workflow:optimize": "node ../version-manager.js optimize-all",
    "workflow:backup": "node ../version-manager.js backup-all"
  },
