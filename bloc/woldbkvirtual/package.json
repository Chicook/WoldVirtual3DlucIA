{
  "name": "woldvirtual-blockchain",
  "version": "1.0.0",
  "description": "Blockchain personalizada para la plataforma WoldVirtual3D con puente BSC y sistema de gas abstraction",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "demo": "node dist/examples/demo.js",
    "bridge-demo": "node dist/examples/bridge-demo.js",
    "gas-demo": "node dist/examples/gas-demo.js",
    "python": "python python_blockchain.py",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:bridge": "jest --testNamePattern='bridge'",
    "test:gas": "jest --testNamePattern='gas'",
    "test:bsc": "jest --testNamePattern='bsc'",
    "test:wcv": "jest --testNamePattern='wcv'",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "postinstall": "npm run build",
    "chain:start": "node start-chain.js",
    "chain:dev": "node start-chain.js dev",
    "chain:prod": "node start-chain.js prod",
    "chain:bridge": "node start-chain.js bridge",
    "chain:gas": "node start-chain.js gas",
    "chain:test": "node start-chain.js test",
    "chain:clean": "node start-chain.js clean",
    "chain:python": "node start-chain.js python",
    "docs": "typedoc src/**/*.ts --out docs",
    "docker:build": "docker build -t woldvirtual3d-blockchain .",
    "docker:run": "docker run -d --name woldvirtual3d -p 8545:8545 -p 8546:8546 -p 8547:8547 woldvirtual3d-blockchain",
    "solidity:compile": "hardhat compile",
    "solidity:test": "hardhat test",
    "solidity:deploy": "hardhat run scripts/deploy.js --network localhost",
    "solidity:deploy-gas": "hardhat run scripts/deploy-gas-system.js --network localhost",
    "rust:build": "cd contracts/rust && cargo build",
    "rust:test": "cd contracts/rust && cargo test",
    "move:build": "cd contracts/move && aptos move compile",
    "move:test": "cd contracts/move && aptos move test",
    "vyper:compile": "cd contracts/vyper && vyper wcv_token.vy",
    "vyper:test": "cd contracts/vyper && python -m pytest"
  },
  "keywords": [
    "blockchain",
    "metaverse",
    "3d",
    "virtual-world",
    "cryptocurrency",
    "defi",
    "nft",
    "gaming",
    "web3",
    "bridge",
    "bsc",
    "binance-smart-chain",
    "wcv",
    "woldcoinvirtual",
    "gas-abstraction",
    "solidity",
    "rust",
    "move",
    "vyper",
    "multi-chain"
  ],
  "author": "WoldVirtual3D Team",
  "license": "MIT",
