{
  "name": "woldvirtual3d-assets-backend",
  "version": "1.0.0",
  "description": "Backend avanzado para gestiÃ³n de assets 3D del metaverso WoldVirtual3D",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "seed": "ts-node src/scripts/seed.ts",
    "seed:full": "npm run seed full",
    "seed:clean": "npm run seed full -- --clean",
    "seed:examples": "npm run seed examples",
    "seed:custom": "npm run seed custom",
    "db:migrate": "typeorm migration:run -d src/database/connection.ts",
    "db:generate": "typeorm migration:generate -d src/database/connection.ts",
    "db:revert": "typeorm migration:revert -d src/database/connection.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean"
  },
  "keywords": [
    "metaverso",
    "3d",
    "assets",
    "backend",
    "api",
    "typescript",
    "express",
    "typeorm",
    "postgresql"
  ],
  "author": "WoldVirtual3D Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17",
    "pg": "^8.11.3",
    "uuid": "^9.0.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "winston": "^3.11.0",
    "@faker-js/faker": "^8.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/node": "^20.8.10",
    "@types/pg": "^8.10.7",
    "@types/uuid": "^9.0.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/jest": "^29.5.8",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "eslint": "^8.52.0",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/woldvirtual3d/assets-backend.git"
  },
  "bugs": {
    "url": "https://github.com/woldvirtual3d/assets-backend/issues"
  },
  "homepage": "https://github.com/woldvirtual3d/assets-backend#readme"
}